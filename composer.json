{
    "name": "tightenco/nova-tighten-themes",
    "description": "A Laravel Nova theme based on Tighten's orange brand color",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\TightenNovaTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\TightenNovaTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
